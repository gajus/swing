    "sinon": "^9.0.1"
  },
  "keywords": [
    "swipe",
    "cards"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "swing",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/swing"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files",
    "lint": "eslint ./src ./test",
    "test": "ava --verbose --serial"
